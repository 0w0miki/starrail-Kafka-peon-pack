    "task.error": {
      "sounds": [
        { "file": "sounds/interesting.wav", "label": "有点意思" },
        { "file": "sounds/not-the-end.wav", "label": "这还…不是结局" },
        { "file": "sounds/oops.wav", "label": "有些难办" },
        { "file": "sounds/found-enemy.wav", "label": "嗯？看他们蠢蠢欲动的样子……" }
      ]
    },
    "input.required": {
      "sounds": [
        { "file": "sounds/time-to-move.wav", "label": "该动手啦" },
        { "file": "sounds/not-in-script.wav", "label": "剧本上没写的，我们决定就好" }
      ]
