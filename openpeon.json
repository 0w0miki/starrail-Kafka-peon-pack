    "task.acknowledge": {
      "sounds": [
        { "file": "sounds/complete-soon.wav", "label": "很快就好" },
        { "file": "sounds/do-all.wav", "label": "索性都干掉吧~" },
        { "file": "sounds/just-in-time.wav", "label": "来得好" },
        { "file": "sounds/start.wav", "label": "那么从现在开始行动由我接手" }
      ]
    },
    "task.complete": {
      "sounds": [
        { "file": "sounds/can-use.wav", "label": "能用" },
        { "file": "sounds/well-done.wav", "label": "做得不错" },
        { "file": "sounds/expected.wav", "label": "毫不意外" },
        { "file": "sounds/complete.wav", "label": "这一次的剧本也顺利完成了" }
      ]
