      ]
    },
    "session.end": {
      "sounds": [
        { "file": "sounds/farewell.wav", "label": "该说再见了…" },
        { "file": "sounds/time-end.wav", "label": "美妙的时光总有尽头" },
        { "file": "sounds/byebye.wav", "label": "拜拜，下一次试着让我意外吧" }
      ]
